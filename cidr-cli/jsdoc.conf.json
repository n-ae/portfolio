{
  "source": {
    "include": ["./index.js"],
    "includePattern": "\\.js$",
    "exclude": ["node_modules/", "docs/", "coverage/", "*.test.js"]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "metadata": {
    "title": "cidr-cli Documentation"
  }
}