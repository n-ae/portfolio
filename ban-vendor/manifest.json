{
    "manifest_version": 3,
    "name": "Ban Vendors",
    "description": "This is a chrome extension written in Go",
    "version": "0.2.0",
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage"
    ],
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "css": [
                "assets/styles.css"
            ],
            "matches": [
                "http://*.yemeksepeti.com/*",
                "https://*.yemeksepeti.com/*"
            ],
            "js": [
                "content_script.js"
            ]
        }
    ]
}
