<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>
  <!-- Simulate XML injection patterns that could confuse parsers -->
  <ItemGroup>
    <PackageReference Include="Normal.Package" Version="1.0.0" />
    <PackageReference Include="&lt;Escaped&gt;" Version="1.0.0" />
    <PackageReference Include="&amp;Ampersand" Version="1.0.0" />
    <PackageReference Include="&quot;Quotes&quot;" Version="1.0.0" />
    <PackageReference Include="&apos;Apostrophe&apos;" Version="1.0.0" />
    <!-- CDATA in attributes (malformed but might be parsed) -->
    <PackageReference Include="Test" Version="<![CDATA[1.0.0]]>" />
    <Content Include="file.txt" CopyToOutputDirectory="<![CDATA[Always]]>" />
    <!-- Nested element-like patterns in attributes -->
    <PackageReference Include="Nested&lt;Element&gt;" Version="1.0.0" />
    <PackageReference Include="Processing&lt;?xml version=&quot;1.0&quot;?&gt;" Version="1.0.0" />
    <!-- Comment-like patterns in attributes -->
    <PackageReference Include="Comment&lt;!-- test --&gt;" Version="1.0.0" />
    <!-- Duplicate these patterns -->
    <PackageReference Include="Normal.Package" Version="1.0.0" />
    <PackageReference Include="&lt;Escaped&gt;" Version="1.0.0" />
    <PackageReference Include="&amp;Ampersand" Version="1.0.0" />
    <PackageReference Include="&quot;Quotes&quot;" Version="1.0.0" />
    <PackageReference Include="&apos;Apostrophe&apos;" Version="1.0.0" />
    <PackageReference Include="Test" Version="<![CDATA[1.0.0]]>" />
    <Content Include="file.txt" CopyToOutputDirectory="<![CDATA[Always]]>" />
    <PackageReference Include="Nested&lt;Element&gt;" Version="1.0.0" />
    <PackageReference Include="Processing&lt;?xml version=&quot;1.0&quot;?&gt;" Version="1.0.0" />
    <PackageReference Include="Comment&lt;!-- test --&gt;" Version="1.0.0" />
  </ItemGroup>
</Project>