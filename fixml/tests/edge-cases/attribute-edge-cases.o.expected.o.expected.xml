<Project>
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <!-- Attribute variations that could confuse normalization -->
    <PackageReference Include="System.Text.Json" Version="6.0.0" />
    <PackageReference Version="6.0.0" Include="System.Text.Json" /><!-- Swapped order -->
      <PackageReference Include="System.Text.Json" Version="6.0.0" PrivateAssets="all" />
      <PackageReference Include="System.Text.Json" PrivateAssets="all" Version="6.0.0" /><!-- Swapped order -->
        <PackageReference Include='System.Text.Json' Version='6.0.0' /><!-- Single quotes -->
          <PackageReference Include="Newtonsoft.Json" Version="13.0.1" ExcludeAssets="runtime" />
          <PackageReference ExcludeAssets="runtime" Include="Newtonsoft.Json" Version="13.0.1" />
          <Content Include="file.txt" CopyToOutputDirectory="Always" />
          <Content CopyToOutputDirectory="Always" Include="file.txt" />
          <Content Include="file.txt" CopyToOutputDirectory="PreserveNewest" />
          <Content Include="file.txt" CopyToOutputDirectory="Never" />
        </ItemGroup>
        <ItemGroup>
          <Reference Include="System.Core" />
          <Reference Include="System.Xml" />
        </ItemGroup>
      </Project>
