<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>
  <!-- Hash collision attack: strings designed to have same hash in common algorithms -->
  <ItemGroup>
    <!-- These strings are designed to collide in common hash functions -->
    <PackageReference Include="FB" Version="1.0.0" />
    <PackageReference Include="Ea" Version="1.0.0" />
    <!-- More collision pairs -->
    <PackageReference Include="polygenelubricants" Version="1.0.0" />
    <PackageReference Include="GuzziParametersBean" Version="1.0.0" />
    <!-- Java String hashCode collision -->
    <PackageReference Include="AaAaAa" Version="1.0.0" />
    <PackageReference Include="AaAaBB" Version="1.0.0" />
    <!-- More collision attempts -->
    <PackageReference Include="BBAaAa" Version="1.0.0" />
    <PackageReference Include="BBAaBB" Version="1.0.0" />
    <!-- Patterns that hash to same value in simple hash functions -->
    <PackageReference Include="abcdefghijklmnop" Version="1.0.0" />
    <PackageReference Include="bcdefghijklmnopa" Version="1.0.0" />
    <!-- Numeric patterns that could cause hash collisions -->
    <PackageReference Include="0123456789" Version="1.0.0" />
    <PackageReference Include="1234567890" Version="1.0.0" />
    <!-- Pattern that could stress hash distribution -->
    <PackageReference Include="aaaaaaaaaa" Version="1.0.0" />
    <PackageReference Include="aaaaaaaaab" Version="1.0.0" />
    <PackageReference Include="aaaaaaaaac" Version="1.0.0" />
    <PackageReference Include="aaaaaaaaad" Version="1.0.0" />
    <PackageReference Include="aaaaaaaaae" Version="1.0.0" />
  </ItemGroup>
</Project>
