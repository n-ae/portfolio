<Project>
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>
  <!-- Timing attack: patterns designed to reveal algorithmic complexity through timing -->
  <ItemGroup>
    <!-- Phase 1: Identical elements (fast hash lookup) -->
    <PackageReference Include="Same.Package.Name" Version="1.0.0" />
    <!-- Phase 2: Nearly identical (forces deep comparison) -->
    <PackageReference Include="Almost.Same.Package.Name" Version="1.0.0" />
    <PackageReference Include="Almost.Same.Package.Name" Version="1.0.1" /><!-- Tiny diff -->
      <PackageReference Include="Almost.Same.Package.Name" Version="1.0.2" /><!-- Tiny diff -->
        <PackageReference Include="Almost.Same.Package.Name" Version="1.0.3" /><!-- Tiny diff -->
          <PackageReference Include="Almost.Same.Package.Name" Version="1.0.4" /><!-- Tiny diff -->
            <!-- Phase 3: Hash collision candidates -->
            <PackageReference Include="AaAa" Version="1.0.0" />
            <PackageReference Include="BBBB" Version="1.0.0" />
            <!-- Phase 4: Progressively longer strings (tests string comparison optimization) -->
            <PackageReference Include="A" Version="1.0.0" />
            <PackageReference Include="AB" Version="1.0.0" />
            <PackageReference Include="ABC" Version="1.0.0" />
            <PackageReference Include="ABCD" Version="1.0.0" />
            <PackageReference Include="ABCDE" Version="1.0.0" />
            <PackageReference Include="ABCDEF" Version="1.0.0" />
            <PackageReference Include="ABCDEFG" Version="1.0.0" />
            <PackageReference Include="ABCDEFGH" Version="1.0.0" />
            <PackageReference Include="ABCDEFGHI" Version="1.0.0" />
            <PackageReference Include="ABCDEFGHIJ" Version="1.0.0" />
            <!-- Phase 5: Common prefix patterns (tests trie optimization) -->
            <PackageReference Include="Microsoft.AspNetCore.Authentication" Version="1.0.0" />
            <PackageReference Include="Microsoft.AspNetCore.Authorization" Version="1.0.0" />
            <PackageReference Include="Microsoft.AspNetCore.Cors" Version="1.0.0" />
            <PackageReference Include="Microsoft.AspNetCore.Diagnostics" Version="1.0.0" />
            <PackageReference Include="Microsoft.AspNetCore.HttpsPolicy" Version="1.0.0" />
            <!-- Phase 6: Worst case scenario for linear search -->
            <PackageReference Include="ZZZ.Last.Package.Alphabetically" Version="1.0.0" />
            <PackageReference Include="ZZY.Second.Last.Package" Version="1.0.0" />
            <PackageReference Include="ZZX.Third.Last.Package" Version="1.0.0" />
          </ItemGroup>
          <!-- Mixed content types to stress heterogeneous collections -->
          <ItemGroup>
            <Content Include="file1.txt" />
            <Compile Include="Class1.cs" />
            <None Include="readme.md" />
          </ItemGroup>
        </Project>
